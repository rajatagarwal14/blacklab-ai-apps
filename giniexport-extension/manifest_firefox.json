{
  "manifest_version": 2,
  "name": "GiniExport - KW CRM Exporter",
  "version": "1.0.0",
  "description": "Export KW CRM opportunities with one click. Powered by Gini AI.",
  "permissions": [
    "*://*.kw.com/*",
    "*://command.kw.com/*",
    "downloads"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.kw.com/*", "*://command.kw.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "giniexport@blacklabai.com",
      "strict_min_version": "109.0"
    }
  }
}
